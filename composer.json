{
	"name": "darkdiesel/pagescroller",
	"type": "phpbb-extension",
	"description": "Page scroller extension add ability to scroll page to the top of page or to the bottom for phpBB 3.1",
	"homepage": "https://github.com/darkdiesel/phphBB-PageScroller",
	"version": "1.0.8.2",
	"time": "2016-06-03",
	"license": "GPL-2.0",
	"authors": [{
			"name": "Igor Peshkov",
			"email": "igor.peshkov@gmail.com",
			"homepage": "https://plus.google.com/+IgorPeshkov",
			"role": "Web Developer"
		}],
	"require": {
		"php": ">=5.3.3"
	},
	"require-dev": {
		"phpbb/epv": "dev-master"
	},
	"extra": {
		"display-name": "Page Scroller",
		"soft-require": {
			"phpbb/phpbb": ">=3.1.2,<3.2.0@dev"
		},
		"version-check": {
			"host": "www.phpbb.com",
			"directory": "/customise/db/extension/page_scroller",
			"filename": "version_check"
		}
	}
}
